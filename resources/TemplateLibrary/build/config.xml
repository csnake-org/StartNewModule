<cruisecontrol>

    <!-- 
    \name TemplateLibrary
    \brief Builds the TemplateLibrary library.
    -->
    <project.default name="TemplateLibrary">

        <!-- Properties -->
        <property name="project.instance_name" value="templateLibrary"/>
        <property name="project.type_src_folder" value="cilabModules"/>
        <property name="project.type_bin_folder" value="library"/>

        <!-- Modificationset: triggers for rebuild -->
        <modificationset>
            <svn/>
            <buildstatus logdir="${main.log_folder}/ThirdParty"/>
        </modificationset>

    </project.default>

</cruisecontrol>
